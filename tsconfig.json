{
  "compileOnSave": false,
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "dynamic-forms": [
        "dist/dynamic-forms"
      ],
      "dynamic-forms/*": [
        "dist/dynamic-forms/*"
      ],
      "custom-form-controls": [
        "dist/custom-form-controls"
      ],
      "custom-form-controls/*": [
        "dist/custom-form-controls/*"
      ],
      "elastic-textarea": [
        "dist/elastic-textarea"
      ],
      "elastic-textarea/*": [
        "dist/elastic-textarea/*"
      ],
      "google-maps": [
        "dist/google-maps"
      ],
      "google-maps/*": [
        "dist/google-maps/*"
      ],
      "profile-card": [
        "dist/profile-card"
      ],
      "profile-card/*": [
        "dist/profile-card/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}